---
import WikiLayout from '../../layouts/WikiLayout.astro';
import TableOfContents from '../../components/TableOfContents.astro';
import InfoBox from '../../components/InfoBox.astro';

const pageTitle = "Shopping Lists";

const headings = [
  { depth: 2, slug: "list-types", text: "Types of Shopping Lists" },
  { depth: 2, slug: "smart-features", text: "Smart List Features" },
  { depth: 2, slug: "shopping-mode", text: "Shopping Mode" },
  { depth: 2, slug: "offline-access", text: "Offline Access" },
  { depth: 2, slug: "list-sharing", text: "List Sharing & Collaboration" },
  { depth: 2, slug: "completion-tracking", text: "Completion Tracking" },
  { depth: 2, slug: "history-analytics", text: "Shopping History & Analytics" }
];
---

<WikiLayout title={pageTitle}>
  <div class="flex justify-between items-center">
    <h1 class="text-3xl font-bold mb-8">{pageTitle}</h1>
    <div class="flex gap-2">
      <a href="#" class="text-sm px-3 py-1 border border-neutral-200 dark:border-neutral-800 rounded-md text-neutral-700 dark:text-neutral-300 hover:bg-neutral-50 dark:hover:bg-neutral-800">
        View history
      </a>
      <a href="#" class="text-sm px-3 py-1 border border-neutral-200 dark:border-neutral-800 rounded-md text-neutral-700 dark:text-neutral-300 hover:bg-neutral-50 dark:hover:bg-neutral-800">
        Discussion
      </a>
    </div>
  </div>

  <TableOfContents headings={headings} />

  <p class="text-lg text-neutral-700 dark:text-neutral-300 mb-6">
    Master the art of efficient shopping with Shopis's intelligent list system. From smart categorization to real-time collaboration, discover how to make every shopping trip more organized and productive.
  </p>

  <h2 id="list-types">Types of Shopping Lists</h2>
  <p>
    Shopis supports various types of shopping lists, each optimized for different shopping scenarios:
  </p>

  <div class="my-6 grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4">
    <div class="border border-neutral-200 dark:border-neutral-800 rounded-lg p-4 bg-gradient-to-br from-green-50 to-emerald-50 dark:from-green-900/20 dark:to-emerald-900/20">
      <h4 class="flex items-center gap-2 font-semibold mb-2 text-green-800 dark:text-green-300">
  <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5" viewBox="0 0 20 20" fill="currentColor">
